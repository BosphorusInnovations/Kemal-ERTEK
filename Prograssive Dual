int sensorPin = A0;    
int minPin = A1;    
int maxPin = A2;
int Max2Pin = A3;    
int testPin = 4;     
int ledPin = 5;
int motorPin = 6;      
int selenoidPin = 7; 
int selenoid2Pin = 9;
int liquidSufPin = 8;
int sensorValue = 0;  
int minValue = 0;
int maxValue = 0;
int buttonState = 0;
int liquidSufVal = 0;
int Max2Value = 0;
int currentcount = 0;
bool Pressure = true;

void setPwmFrequency(int pin, int divisor) {
  byte mode;
  if (pin == 5 || pin == 6 || pin == 9 || pin == 10) {
    switch (divisor) {
      
      case 1: mode = 0x01; break;
      case 8: mode = 0x02; break;
      case 64: mode = 0x03; break;
      case 256: mode = 0x04; break;
      case 1024: mode = 0x05; break;
      default: return;
    }
    if (pin == 5 || pin == 6) {
      TCCR0B = TCCR0B & 0b11111000 | mode;
    } else {
      TCCR1B = TCCR1B & 0b11111000 | mode;
    }
  } else if (pin == 3 || pin == 11) {
    switch (divisor) {
      case 1: mode = 0x01; break;
      case 8: mode = 0x02; break;
      case 32: mode = 0x03; break;
      case 64: mode = 0x04; break;
      case 128: mode = 0x05; break;
      case 256: mode = 0x06; break;
      case 1024: mode = 0x07; break;
      default: return;
    }
    TCCR2B = TCCR2B & 0b11111000 | mode;
  }
}
void setup() {
    pinMode(ledPin, OUTPUT);
    pinMode(selenoidPin, OUTPUT);
    pinMode(selenoid2Pin, OUTPUT);
    pinMode(LED_BUILTIN, OUTPUT);
    pinMode(Max2Pin, INPUT);
    pinMode(motorPin, OUTPUT);
    pinMode(sensorPin, INPUT);
    pinMode(minPin, INPUT);
    pinMode(maxPin, INPUT);
    pinMode(testPin, INPUT);
    pinMode(liquidSufPin, INPUT);
      setPwmFrequency(5, 64);
      setPwmFrequency(6, 1);
      Serial.begin(9600);
}
void loop() {
  if(currentcount == 0){
    digitalWrite(ledPin, HIGH); 
    digitalWrite(motorPin, HIGH);
    digitalWrite(selenoidPin, HIGH);
    digitalWrite(selenoid2Pin, HIGH);
    digitalWrite(LED_BUILTIN, HIGH);
      delay(5000);            
       digitalWrite(selenoid2Pin, LOW);
       digitalWrite(ledPin, LOW);
       digitalWrite(motorPin, LOW);
       digitalWrite(selenoidPin, LOW);
       digitalWrite(LED_BUILTIN, LOW);
          currentcount=1;
}
  buttonState = digitalRead(testPin);
  if (buttonState == HIGH) {
          digitalWrite(selenoid2Pin, HIGH);

    digitalWrite(motorPin, HIGH);
    digitalWrite(ledPin, HIGH);
    digitalWrite(selenoidPin, HIGH);
    digitalWrite(LED_BUILTIN, HIGH); 
   delay(5000);   
    digitalWrite(selenoid2Pin, LOW);
    digitalWrite(motorPin, LOW);
    digitalWrite(ledPin, LOW);
    digitalWrite(selenoidPin, LOW);
    digitalWrite(LED_BUILTIN, LOW); 
  }
   liquidSufVal = digitalRead(liquidSufPin);
   if (liquidSufVal == HIGH) {
     digitalWrite(ledPin, HIGH); 
     digitalWrite(LED_BUILTIN, HIGH); 
     delay(50000);                
     digitalWrite(ledPin, LOW);  
     digitalWrite(LED_BUILTIN, LOW);
     delay(50000);    
  }
  sensorValue = analogRead(sensorPin);
  minValue = analogRead(minPin);
              if(sensorValue >= 275){
          Pressure = true;
              }
          else{
          Pressure = false;
          }
  maxValue = analogRead(maxPin);
       if (minValue <= maxValue && sensorValue > minValue && sensorValue < maxValue && Pressure) {
       int val = analogRead(sensorPin);
       val = map(val, minValue, maxValue, 75, 255);
       analogWrite(motorPin, val);
       analogWrite(ledPin, val);
       analogWrite(LED_BUILTIN, val);
       analogWrite(selenoidPin, 255);
    } 
    else if(sensorValue >= maxValue && sensorValue >= minValue && minValue <= maxValue && Pressure){
       analogWrite(motorPin, 255);
       analogWrite(ledPin, 255);
       analogWrite(LED_BUILTIN, 255);
       analogWrite(selenoidPin, 255);
             Max2Value = analogRead(Max2Pin);
       if(sensorValue >= Max2Value && Max2Value > maxValue){
        analogWrite(selenoid2Pin, 255);
               delay(10000);
               digitalWrite(LED_BUILTIN, LOW);
               digitalWrite(ledPin, LOW);
               delay(10000);
               digitalWrite(ledPin,HIGH);
               digitalWrite(LED_BUILTIN,HIGH);
        }
        else{
        analogWrite(selenoid2Pin, 0);
          }
      }
  else {
       analogWrite(selenoid2Pin, 0);
       analogWrite(selenoidPin, 0);
       analogWrite(motorPin, 0);
       analogWrite(ledPin, 0);  
       analogWrite(LED_BUILTIN, 0);
       }
}
